# 环境配置文件示例
# Environment Configuration Example
# 
# 使用方法：
# 1. 复制此文件到项目根目录
# 2. 重命名为对应的环境文件：.env.dev, .env.staging, .env.prod
# 3. 根据实际需求修改配置值

# ===========================================
# 应用基础信息 (App Basic Info)
# ===========================================
APP_NAME=Sky ElderCare Family
APP_VERSION=1.0.0
BUILD_NUMBER=1
PACKAGE_NAME=com.example.zjs_flutter_template_3

# ===========================================
# 网络配置 (Network Configuration)
# ===========================================
API_BASE_URL=https://api.example.com
CONNECT_TIMEOUT=30000
RECEIVE_TIMEOUT=30000
SEND_TIMEOUT=30000
MAX_RETRIES=3
RETRY_DELAY=1000

# 缓存配置 (Cache Configuration)
ENABLE_CACHE=true
CACHE_MAX_AGE=300
CACHE_MAX_SIZE=10485760

# ===========================================
# 功能开关 (Feature Flags)
# ===========================================
DEBUG=false
ENABLE_DEBUG_TOOLS=false
ENABLE_DEBUG_BANNER=false
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_BIOMETRIC=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DARK_MODE=true
ENABLE_DYNAMIC_COLORS=true
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_BETA_FEATURES=false

# 日志配置 (Logging Configuration)
LOG_LEVEL=info
ENABLE_FILE_LOGGING=false
ENABLE_REMOTE_LOGGING=true

# ===========================================
# 安全配置 (Security Configuration)
# ===========================================
JWT_SECRET=your_jwt_secret_key_32_characters_long_minimum
JWT_EXPIRY=3600
REFRESH_TOKEN_EXPIRY=604800
ENCRYPTION_KEY=your_encryption_key_32_chars_long
ENCRYPTION_IV=your_iv_16_chars

# SSL配置 (SSL Configuration)
ENABLE_SSL_PINNING=true
ENABLE_CERTIFICATE_TRANSPARENCY=true

# 会话配置 (Session Configuration)
SESSION_TIMEOUT=1800
MAX_LOGIN_ATTEMPTS=5
LOCKOUT_DURATION=900

# 数据库配置 (Database Configuration)
DATABASE_URL=
DATABASE_ENCRYPTION_KEY=

# ===========================================
# 第三方服务 API 密钥 (Third-party API Keys)
# ===========================================
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
FIREBASE_API_KEY=your_firebase_api_key
ONESIGNAL_APP_ID=your_onesignal_app_id

# ===========================================
# 环境标识 (Environment Identifier)
# ===========================================
ENVIRONMENT=production
